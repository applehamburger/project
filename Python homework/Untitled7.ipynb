    "#间隔生成列表3.21\n",
    "def merge_lists(a, b):\n",
    "    \"\"\"\n",
    "    将两个相同长度的list，间隔插入，生成并返回一个新的list。\n",
    "    :param a: 第一个list\n",
    "    :param b: 第二个list\n",
    "    :return: 生成的新list\n",
    "    \"\"\"\n",
    "    result = []\n",
    "    for i in range(len(a)):\n",
    "        result.append(a[i])\n",
    "        result.append(b[i])\n",
    "    return result"
   ]
  }
